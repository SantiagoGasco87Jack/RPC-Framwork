{
  "name": "twirpscript",
  "version": "0.0.66",
  "description": "Protobuf RPC framework for JavaScript and TypeScript",
  "license": "MIT",
  "author": "Santiago <devsantiago87@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/SantiagoGasco87Jack/twirpscript"
  },
  "type": "module",
  "main": "./cjs/index.cjs",
  "module": "./index.js",
  "bin": {
    "twirpscript": "cli/index.js"
  },
  "sideEffects": false,
  "types": "index.d.ts",
  "dependencies": {
    "protoscript": "0.0.14"
  },
  "keywords": [
    "protobuf",
    "protocol buffers",
    "rpc",
    "twirp",
    "typescript"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "node": {
        "import": "./index-node.js",
        "require": "./cjs/index-node.cjs"
      },
      "import": "./index.js",
      "module": "./index.js",
      "require": "./cjs/index.cjs",
      "default": "./index.js"
    }
  }
}
